{
  "expo": {
    "name": "Appstral",
    "label": "Appstral",
    "slug": "appstral",
    "icon": "./assets/icons/android-icon.png",
    "version": "1.0.0",
    "orientation": "portrait",
      "splash": {
      "image": "./assets/icons/splash.png", 
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon-foreground.png",
        "backgroundImage": "./assets/icons/adaptive-background.png"
      },
      "package": "com.tomsnxw.appstral",
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": ["com.android.vending.BILLING"]
    },
    "extra": {
      "eas": {
        "projectId": "3b2375b3-d752-443c-a63d-afacea9008c0"
      }
    },
    "plugins": [
      [
        "react-native-iap",
        {
          "paymentProvider": "both"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/notification-icon.png", 
          "color": "#023c69"
        }
      ]
    ],
    "owner": "tomsnxw"
  }
}
